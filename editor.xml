<application>
  <component name="AndroidFoldingSettings">
    <option name="collapseAndroidStrings" value="false" />
    <option name="COLLAPSE_ANDROID_TEXT" value="false" />
  </component>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
    <option name="COLLAPSE_FILE_HEADER" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Source Code Pro" />
    <option name="LINE_SPACING" value="1.0" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="false" />
    <option name="SHOW_INTENTION_BULB" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="BIDI_TEXT_DIRECTION" value="LTR" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="enabled" value="false" />
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseConstructorGenericParameters" value="false" />
    <option name="collapseI18nMessages" value="false" />
    <option name="collapseLambdas" value="false" />
    <option name="collapseOneLineMethods" value="false" />
    <option name="collapseSuppressWarnings" value="false" />
  </component>
  <component name="OCCodeFoldingSettings">
    <option name="collapseConditionallyNotCompiled" value="false" />
    <option name="collapseNewLineBeforeLBrace" value="false" />
    <option name="collapseTemplateParamList" value="false" />
    <option name="COLLAPSE_TEMPLATE_PARAM_LIST" value="false" />
    <option name="COLLAPSE_CONDITIONALLY_NOT_COMPILED" value="false" />
