<application>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bTODO.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bHACK.*">
      <option name="FOREGROUND" value="ff0080" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff0080" />
    </pattern>
  </component>
</application>