<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bTODO.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bHACK.*">
      <option name="FOREGROUND" value="ff0080" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff0080" />
    </pattern>
  </component>
</application>