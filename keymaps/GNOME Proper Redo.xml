<keymap version="1" name="GNOME Proper Redo" parent="Default for GNOME">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="ExpressionTypeInfo" />
  <action id="ExternalSystem.ProjectRefreshAction" />
  <action id="PlaybackLastMacro">
    <keyboard-shortcut first-keystroke="shift ctrl p" />
  </action>
  <action id="StartStopMacroRecording">
    <keyboard-shortcut first-keystroke="shift ctrl o" />
  </action>
</keymap>